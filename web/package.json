{
  "name": "@yourgreatapp/web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^16.11.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "dotenv": "^16.0.0",
    "i18next-browser-languagedetector": "^6.1.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
