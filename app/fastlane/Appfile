# iOS
app_identifier(ENV['APP_IDENTIFIER']) # The bundle identifier of your app
apple_id("fastlane@stormotion.io") # Your Apple email address
 
itc_team_id(ENV['ITC_TEAM_ID']) # App Store Connect Team ID
team_id(ENV['TEAM_ID']) # Developer Portal Team ID
 
# Android
json_key_file("android/play_console.json") # Path to the json secret file - Follow https://docs.fastlane.tools/actions/supply/#setup to get one
package_name(ENV['PACKAGE_NAME']) # e.g. com.krausefx.app
